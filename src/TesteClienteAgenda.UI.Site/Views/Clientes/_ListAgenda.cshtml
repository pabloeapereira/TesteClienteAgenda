@model TesteClienteAgenda.Application.ViewModels.ClienteViewModel
    <h3>Agendas</h3>
<table class="table">
    <thead>
    <tr>
        <th>
            <label class="control-label">Número do Título</label>
        </th>
        <th>
            <label class="control-label">Valor Bruto</label>
        </th>
        <th>
            <label class="control-label">Valor Líquido</label>
        </th>
        <th>
            <label class="control-label">Taxa</label>
        </th>
        <th>
            <label class="control-label">Data de Liquidação</label>
        </th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Agendas)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.NumeroTitulo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorBruto)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ValorLiquido)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Taxa)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DataLiquidacao)
            </td>
        </tr>
    }
    </tbody>
</table>